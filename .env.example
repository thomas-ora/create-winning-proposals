# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Supabase Configuration (Required)
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# App Configuration (Optional)
VITE_APP_NAME="Proposal Generation System"
VITE_SUPPORT_EMAIL=support@yourcompany.com
VITE_COMPANY_NAME="Your Company Name"

# Development Settings (Optional)
VITE_DEBUG_MODE=false
VITE_ENABLE_ANALYTICS=true

# API Configuration (Optional - uses defaults if not set)
VITE_API_BASE_URL=https://your-project-id.supabase.co/functions/v1
VITE_MAX_PROPOSALS_PER_USER=100
VITE_PROPOSAL_EXPIRY_DAYS=30

# Branding (Optional)
VITE_PRIMARY_COLOR=#7B7FEB
VITE_LOGO_URL=https://your-domain.com/logo.png
VITE_FAVICON_URL=https://your-domain.com/favicon.ico

# Contact Information (Optional)
VITE_CONTACT_EMAIL=contact@yourcompany.com
VITE_CONTACT_PHONE=+1-555-0123
VITE_SUPPORT_URL=https://your-domain.com/support

# Feature Flags (Optional)
VITE_ENABLE_PDF_DOWNLOAD=true
VITE_ENABLE_PASSWORD_PROTECTION=true
VITE_ENABLE_ANALYTICS_TRACKING=true
VITE_ENABLE_EMAIL_NOTIFICATIONS=false

# Social Links (Optional)
VITE_SOCIAL_TWITTER=https://twitter.com/yourcompany
VITE_SOCIAL_LINKEDIN=https://linkedin.com/company/yourcompany
VITE_SOCIAL_GITHUB=https://github.com/yourcompany

# Integration Settings (Optional)
VITE_ZAPIER_WEBHOOK_URL=https://hooks.zapier.com/hooks/catch/your-webhook
VITE_SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your-webhook
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX

# Security Settings (Optional)
VITE_SESSION_TIMEOUT=3600
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=pdf,doc,docx,jpg,png

# Performance Settings (Optional)
VITE_CACHE_DURATION=3600
VITE_REQUEST_TIMEOUT=30000
VITE_RETRY_ATTEMPTS=3

# Deployment Settings (Optional)
VITE_ENVIRONMENT=production
VITE_VERSION=1.0.0
VITE_BUILD_DATE=2024-07-14